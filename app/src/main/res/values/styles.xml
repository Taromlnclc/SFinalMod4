<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomTable">
        <item name="android:paddingStart">12sp</item>
        <item name="android:paddingEnd">12sp</item>
        <item name="android:layout_column">1</item>
    </style>
    <style name="CustomTable.Header">
        <item name="android:paddingTop">20sp</item>
        <item name="android:paddingBottom">20sp</item>
    </style>
    <style name="CustomTable.row">
        <item name="android:paddingTop">12sp</item>
        <item name="android:paddingBottom">12sp</item>
    </style>

    <style name="textLight" parent="Widget.AppCompat.TextView">
        <!-- Establecer el color de fondo del TextView -->
        <item name="android:textColor">#2D485E</item>
    </style>
    <style name="textDark" parent="Widget.AppCompat.TextView">
        <!-- Establecer el color de fondo del TextView -->
        <item name="android:textColor">#B8B8B8</item>
    </style>

</resources>